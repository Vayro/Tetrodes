function summonSample(){
switch(summon)
{
case obj_I:
obj_displaySample.drawSample="straight";
break;

case obj_square:
obj_displaySample.drawSample="square";
break;

case obj_t:
obj_displaySample.drawSample="t";
break;

case obj_L:
obj_displaySample.drawSample="L";
break;

case obj_reverseL:
obj_displaySample.drawSample="rL";
break;

case obj_Skew:
obj_displaySample.drawSample="skew";
break;

case obj_reverseSkew:
obj_displaySample.drawSample="rSkew";
break;

default:
//changeObj=obj_world;

}
}
